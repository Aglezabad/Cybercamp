#!/usr/bin/env node
var debug = require('debug')('Cybercamp');
var app = require('../app');
var config = require('../config.json');

app.set('port', process.env.PORT || config.app.port || 5555);

// Web interna de gestion
var server = app.listen(app.get('port'), function() {
	debug('Express server listening on port ' + server.address().port);
});

// Proxy HTTP
var httpProxy = require('http-proxy');
var http = require('http');
var proxy = httpProxy.createProxyServer({});
var virustotal = require('virustotal.js');
virustotal.setKey('07619252310403a90085da588803b11e98c23e3ec899b12fc5cc588507f82ec0');
var server = http.createServer(function(req, res) {


	virustotal.getUrlReport(req.url, function (err, res_vt) {
		if (err) {
			console.error(err);
			res.statusCode = 500;
			if (err.json.response_code == 0) {
				res.write('<pre><p>No ha sido verificada</p></pre>');
			}
			res.write('<pre><p>'+err.json.verbose_msg+'</p></pre>');
			res.end();
		} else {
			console.log("Info en VT.")
			if(res_vt.positives >= 3) {
				console.log("Virus detectado")
				console.log(res_vt);
				res.write('<h1>Se ha detectado un virus, ha sido bloqueado!</h1>')
				res.write('<img src="data:image/jpg;base64,"/>');
				res.write('<pre>'+res_vt.positives+'/'+res_vt.total+'</pre>')
				res.end();
			}
			else {
				console.log("Not bad.")
				var proxy = http.createClient(80, req.headers['host'])

				var proxy_request = proxy.request(req.method, req.url, req.headers);
				proxy_request.addListener('response', function (proxy_response) {
					proxy_response.addListener('data', function(chunk) {
						res.write(chunk, 'binary');
					});
					proxy_response.addListener('end', function() {
						res.end();
					});
					res.writeHead(proxy_response.statusCode, proxy_response.headers);
				});
				req.addListener('data', function(chunk) {
					proxy_request.write(chunk, 'binary');
				});
				req.addListener('end', function() {
					proxy_request.end();
				});

			}
		}
	}); 

	/* Primer filtro de virus.
	virustotal.getUrlReport(req.url, function (err, res_vt) {
		if (err) {
			console.error(err);
			res.statusCode = 500;
			res.end(err);
			return
		}
		if(res_vt.positives > 5) {
			res.write('<img src="http://makeameme.org/media/created/virus-virus-everywhere.jpg"/>');
			res.end();
		}
		else {
			proxy.web(req, res);
		}
	}); */

	// proxy.web(req, res, { target: 'http://gul.es' });

	/* var auth = req.headers['authorization']; // auth is in base64(username:password) so we need to decode the base64
	console.log("Authorization Header is: ", auth);
	 
